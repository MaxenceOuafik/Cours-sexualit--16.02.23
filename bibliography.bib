  title = {Patterns of {{Sexual Practices}}, {{Sexually Transmitted Infections}} and {{Other Genital Infections}} in {{Women Who Have Sex}} with {{Women Only}} ({{WSWO}}), {{Women Who Have Sex}} with {{Men Only}} ({{WSMO}}) and {{Women Who Have Sex}} with {{Men}} and {{Women}} ({{WSMW}}): {{Findings}} from a {{Sexual Health Clinic}} in {{Melbourne}}, {{Australia}}, 2011â€“2019},
  shorttitle = {Patterns of {{Sexual Practices}}, {{Sexually Transmitted Infections}} and {{Other Genital Infections}} in {{Women Who Have Sex}} with {{Women Only}} ({{WSWO}}), {{Women Who Have Sex}} with {{Men Only}} ({{WSMO}}) and {{Women Who Have Sex}} with {{Men}} and {{Women}} ({{WSMW}})},
  author = {Engel, Jaimie L. and Fairley, Christopher K. and Greaves, Kate E. and Vodstrcil, Lenka A. and Ong, Jason J. and Bradshaw, Catriona S. and Chen, Marcus Y. and Phillips, Tiffany R. and Chow, Eric P. F.},
  year = {2022},
