  pmid = {31298497},
  keywords = {âœ…,ðŸ³ï¸â€âš§ï¸,ðŸ³ï¸â€ðŸŒˆ,GAHT,HIV/AIDS,Infectious diseases,PrEP,STI,Transgender women},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\BUXP8M7Z\\Hiransuthikul et al. - 2019 - Drug-drug interactions between feminizing hormone .pdf}
