  author = {Crowley, Fionnuala and Mihalopoulos, Meredith and Gaglani, Simita and Tewari, Ashutosh K. and Tsao, Che-Kai and Djordjevic, Miroslav and Kyprianou, Natasha and Purohit, Rajveer S. and Lundon, Dara J.},
  year = {2023},
