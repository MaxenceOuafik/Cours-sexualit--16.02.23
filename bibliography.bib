  keywords = {ðŸ³ï¸â€âš§ï¸,ðŸ³ï¸â€ðŸŒˆ,ðŸ“–,Healthcare access,Qualitative study},
  file = {C\:\\Users\\maxen\\Zotero\\storage\\FUBFI4PK\\Snelgrove et al. - 2012 - â€œCompletely out-at-seaâ€ with â€œtwo-gender medicineâ€.pdf}
}

