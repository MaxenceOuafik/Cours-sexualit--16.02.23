@article{goodman2022,
  title = {Human {{Papillomavirus Vaccine Impact}} and {{Effectiveness}} in {{Six High-Risk Populations}}: {{A Systematic Literature Review}}},
  shorttitle = {Human {{Papillomavirus Vaccine Impact}} and {{Effectiveness}} in {{Six High-Risk Populations}}},
  author = {Goodman, Elizabeth and Reuschenbach, Miriam and Kaminski, Allysen and Ronnebaum, Sarah},
  year = {2022},
